{
    "title": "User",
    "description": "List of Users",
    "pageType": "view",
    "sendIdWithCreate": true,
    "urlFields": ["_id"],
    "fields": [{
        "name": "Name",
        "isSort": "",
        "tableRef": "name"
    }, {
        "name": "Blogger",
        "isSort": "",
        "tableRef": "blogger"
    }, {
        "name": "Is Popular Blogger",
        "isSort": "",
        "tableRef": "isPopulars",
        "type": "toggle",
        "url": "user/editData"
    }],
    "action": [{
        "name": "edit",
        "icon": "fa-pencil",
        "buttonClass": "btn-primary",
        "type": "page",
        "action": "editUser",
        "fieldsToSend": {
            "_id": "_id"
        }
    }, {
        "name": "delete",
        "icon": "fa-trash",
        "buttonClass": "btn-danger",
        "type": "apiCallConfirm",
        "title": "Delete User",
        "content": "Are you sure you want to delete User?",
        "api": "user/delete",
        "fieldsToSend": {
            "name": "_id",
            "value": "_id"
        }
    }],
    "button": [{
        "name": "Create",
        "icon": "plus",
        "class": "btn-success",
        "type": "page",
        "action": "createUser"
    }],
    "apiCall": {
        "url": "User/getBloggers",
        "params": "_id"
    }
}